  "scripts": {
    "lint": "eslint --ext=.js,.jsx,.tx,.tsx --max-warnings=0 src",
    "start": "craco start",
    "build": "craco build",
    "test": "react-scripts test --testPathIgnorePatterns=src/__tests__/_helpers --testPathIgnorePatterns=src/__tests__/_data --testPathIgnorePatterns=src/__tests__/_mocks ",
    "eject": "react-scripts eject",
    "prettier": "prettier --check src",
    "format": "prettier --write src"
  },
  "jest": {
    "resetMocks": false,
    "transformIgnorePatterns": [
      "/node_modules/(?!antd|@ant-design|rc-.+?|@babel/runtime).+(js|jsx)$"
    ]
